<featureModel>
	<struct>
		<and abstract="true" mandatory="true" name="camera">
			<or mandatory="true" name="goal">
				<feature name="landscape"/>
				<feature name="portrait"/>
				<feature name="sports"/>
			</or>
			<feature name="lightweight"/>
			<feature name="exchangelens"/>
			<feature name="waterproof"/>
		</and>
	</struct>
	<constraints>
		<rule>
			<imp>
				<var>sports</var>
				<var>waterproof</var>
			</imp>
		</rule>
	</constraints>
</featureModel>